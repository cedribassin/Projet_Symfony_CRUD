{% extends 'base.html.twig' %}

{% block title %}Page des aliments{% endblock %}
{% block monTitre %}Liste de tous les types{% endblock %}
{% block body %}
<div class="row mt-3">
    {% for type in types %}
        <div class="col text-center">
            <h2>{{type.libelle}}</h2>
            <img src="{{asset('images/types/' ~ type.image)}}" class="imageAliments">
        </div>
    {% endfor %}
</div>
{% endblock %}
